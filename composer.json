{
    "name": "andytruong/at_base",
    "type": "drupal-module",
    "description": "Provide some more API for developer to work with Drupal",
    "keywords": ["Drupal", "drush", "dependency injection", "container", "config", "api", "autoloader"],
    "homepage": "http://v3k.net/",
    "license": "GPL2",
    "authors": [
        {
            "name": "Andy Truong",
            "email": "thehongtt@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "mustangostang/spyc": "0.5.*@dev",
        "pimple/pimple": "2.0.*@dev",
        "twig/twig": "1.15.*@dev",
        "symfony/expression-language": "2.5.*@dev"
    },
    "autoload": {
        "psr-4": { "Drupal\\at_base": "lib" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.x-2.x"
        }
    }
}
